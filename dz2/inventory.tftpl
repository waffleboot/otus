[all:vars]
ansible_user=${user}
ansible_ssh_private_key_file=id_rsa

[server]
${public_ip}

[clients]
%{ for addr in clients ~}
${addr}
%{ endfor ~}

[clients:vars]
ansible_ssh_common_args='-oProxyCommand="ssh -W %h:%p -q -i id_rsa ${user}@${public_ip}" -i id_rsa'
clients='${join(" ",clients)}'
server=${public_ip}
